import values.*;
import show.*;

symbol fib;
overload #fib('n) = #fib('n - 1) + #fib('n - 2);
overload #fib(1) = 1;
overload #fib(0) = 0;

main() {
  var f = #20;

  static for (n in ..#intValues(f)) {
    show(#fib(n));
  }

  show("---");

  var vals = [..#mapValues(#fib, ..#intValues(f))];

  show(#Type([vals]));
}
